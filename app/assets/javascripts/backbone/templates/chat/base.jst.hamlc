-#%li.clearfix{class: "#{if @model.me() then "me"}", style: "color: #{@model.get("userColor")}"}
.thumbnail{style: "background-color: #{@model.get("userColor")}"}
.chat-body.clearfix
  .header
    %strong.primary-font
      = @model.get("userName")
    %small.text-muted
      %span.fa.fa-clock-o
      %span.clock
  %p
    = @model.get("text")
